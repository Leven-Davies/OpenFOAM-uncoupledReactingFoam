FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0";
    object      O2;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0.23;

boundaryField
{
    inletLow
    {
        type            fixedValue;
        value           nonuniform List<scalar> 0();
    }
    inletJet
    {
        type            fixedValue;
        value           nonuniform List<scalar> 0();
    }
    inletHigh
    {
        type            fixedValue;
        value           nonuniform List<scalar> 0();
    }
    outlet
    {
        type            inletOutlet;
        inletValue      nonuniform List<scalar> 0();
        value           nonuniform List<scalar> 0();
    }
    upperWall
    {
        type            inletOutlet;
        inletValue      nonuniform List<scalar> 0();
        value           nonuniform List<scalar> 0();
    }
    lowerWall
    {
        type            inletOutlet;
        inletValue      uniform 0.23;
        value           uniform 0.23;
    }
    frontAndBack
    {
        type            empty;
    }
    procBoundary5to0
    {
        type            processor;
        value           uniform 0.23;
    }
    procBoundary5to2
    {
        type            processor;
        value           uniform 0.23;
    }
    procBoundary5to4
    {
        type            processor;
        value           uniform 0.23;
    }
    procBoundary5to6
    {
        type            processor;
        value           uniform 0.23;
    }
}


