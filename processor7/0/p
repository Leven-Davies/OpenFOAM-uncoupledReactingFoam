FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 100000;

boundaryField
{
    inletLow
    {
        type            zeroGradient;
    }
    inletJet
    {
        type            zeroGradient;
    }
    inletHigh
    {
        type            zeroGradient;
    }
    outlet
    {
        type            fixedValue;
        value           nonuniform List<scalar> 0();
    }
    upperWall
    {
        type            totalPressure;
        rho             rho;
        psi             none;
        gamma           1;
        p0              nonuniform List<scalar> 0();
        value           nonuniform List<scalar> 0();
    }
    lowerWall
    {
        type            totalPressure;
        rho             rho;
        psi             none;
        gamma           1;
        p0              uniform 100000;
        value           uniform 100000;
    }
    frontAndBack
    {
        type            empty;
    }
    procBoundary7to3
    {
        type            processor;
        value           uniform 100000;
    }
    procBoundary7to6
    {
        type            processor;
        value           uniform 100000;
    }
}


