FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 1000;

boundaryField
{
    inletLow
    {
        type            fixedValue;
        value           nonuniform List<scalar> 0();
    }
    inletJet
    {
        type            fixedValue;
        value           nonuniform List<scalar> 0();
    }
    inletHigh
    {
        type            fixedValue;
        value           nonuniform List<scalar> 0();
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform 300;
        value           uniform 300;
    }
    upperWall
    {
        type            inletOutlet;
        inletValue      nonuniform List<scalar> 0();
        value           nonuniform List<scalar> 0();
    }
    lowerWall
    {
        type            inletOutlet;
        inletValue      uniform 300;
        value           uniform 300;
    }
    frontAndBack
    {
        type            empty;
    }
    procBoundary4to0
    {
        type            processor;
        value           uniform 1000;
    }
    procBoundary4to1
    {
        type            processor;
        value           uniform 1000;
    }
    procBoundary4to5
    {
        type            processor;
        value           uniform 1000;
    }
}


