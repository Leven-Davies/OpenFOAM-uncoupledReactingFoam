FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 1e5;

boundaryField
{
    inletJet
    {
        type            zeroGradient;
    }

    inletLow
    {
        type            zeroGradient;
    }

    inletHigh
    {
        type            zeroGradient;
    }

    outlet
    {
        type  fixedValue;
        value uniform 1e5;
    }

    upperWall
    {
        type            totalPressure;
        p0              uniform 1e5; // Ambient pressure
        value           uniform 1e5;
    }

    lowerWall
    {
        type            totalPressure;
        p0              uniform 1e5; // Ambient pressure
        value           uniform 1e5;
    }

    frontAndBack
    {
        type            empty;
    }
}


// ************************************************************************* //
